{% extends "BackofficeBaseBundle::Layout/AdminLayout.html.twig" %}

{% block navActive %}etablissement{% endblock %}

{% block contentHeader %}
<h1>
    Administration des etablissements
</h1>
{% endblock %}

{% block content %}

<div class="col-md-12 col-sm-12 col-xs-12">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Création d'établissements</h3>
        </div>
        <div class="box-body">
            {{ form(form, {'attr': {'id': 'form_lieux_add'}} ) }}
        </div>
        <div class="box-footer">
            <button type="submit" form="form_lieux_add" class="btn btn-primary pull-right">Valider</button>
        </div>
    </div>
</div>
{% endblock %}
